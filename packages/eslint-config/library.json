{
  "extends": ["eslint:recommended", "turbo"],
  "plugins": ["only-warn"],
  "globals": {
    "React": true,
    "JSX": true
  },
  "env": {
    "node": true
  },
  "settings": {},
  "ignorePatterns": [".*.js", "node_modules/", "dist/"],
  "overrides": [
    {
      "files": ["*.js?(x)", "*.ts?(x)"]
    }
  ]
}
